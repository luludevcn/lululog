---
import Social from "./Social.astro";
---



<div class="h-14"></div>
<div class="flex fixed left-0 bottom-10 gap-4 leading-10 opacity-80 font-mono w-full bg-white font-bold divide-x-2 justify-center">
  <Social url="https://nextjscn.org/" title="NextJS"></Social>
  <Social url="https://astro.build/" title="Astro"></Social>
  <Social url="https://nutui.jd.com/" title="Nutui"></Social>
  <Social url="https://www.tailwindcss.cn/" title="TailwindCss"></Social>
  <Social url="https://react.dev/" title="React"></Social>
  <Social url="https://nodejs.cn/en" title="NodeJS"></Social>
  <Social url="https://www.tslang.cn/" title="TypeScript"></Social>
</div>
